{
	  "version": "0.1.1",
	  "depends": ["uv"],
	  "url": "https://github.com/microsoft/markitdown/archive/refs/tags/v0.1.1.tar.gz",
	  "bin": [["uv.exe", "markitdown", "markitdown"]],
	  "post_install": [
	  "Start-Process -FilePath \"uv.exe\" -ArgumentList \"venv\" -WorkingDirectory \"$dir\\markitdown-0.1.1\" -NoNewWindow -Wait ",
	  "Start-Process -FilePath \"uv.exe\" -ArgumentList \" pip install -e 'packages/markitdown[all]'\" -WorkingDirectory \"$dir\\markitdown-0.1.1\" -NoNewWindow -Wait "
	  ]
}
